// nav
function openNav() {
    document.getElementById("mercMenu").style.width = "calc(100% - 60px)";
    document.getElementById("mercMenu").style.right = "0px";
}

function closeNav() {
    document.getElementById("mercMenu").style.width = "0";
    document.getElementById("mercMenu").style.right = "-60px";
}
